{
  "name": "recall-calendar-v2-apis-demo",
  "version": "1.0.0",
  "description": "This application demonstrates how a Recall API consumer can integrate with the V2 Calendar APIs which are designed to be server side friendly.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:headless": "playwright test --headless",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "build": "echo 'No build step required for server-side application'",
    "start": "NODE_ENV=production node index.js",
    "start:worker": "NODE_ENV=production node worker/index.js",
    "dev": "NODE_ENV=development node index.js",
    "dev:watch": "NODE_ENV=development nodemon index.js",
    "dev:worker": "NODE_ENV=development nodemon worker/index.js",
    "migrate": "node run-migrations.js",
    "disconnect-v1-bot": "node disconnect-v1-bot.js",
    "stop-v1-scheduler": "node stop-v1-scheduler.js",
    "check-v2-bot-scheduling": "node check-v2-bot-scheduling.js"
  },
  "author": "Dhruv Parmar",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.974.0",
    "@aws-sdk/s3-request-presigner": "^3.525.0",
    "bull": "^4.10.4",
    "console-stamp": "^3.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.10",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-authentication": "^0.3.2",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "^3.3.1",
    "redis": "^4.6.12",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "umzug": "^3.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,ejs,sql"
  }
}
