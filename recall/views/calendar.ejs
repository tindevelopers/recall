<%- include('partials/header') -%>
<div class="min-h-full">
  <%- include('partials/navigation') -%> <%- include('partials/notice', {
  notice: notice }) -%>
  <div class="py-10">
    <main>
      <div class="mx-auto max-w-7xl sm:px-6 lg:px-8">
        <div class="flex flex-col space-y-6">
          <!-- Calendar Settings -->
          <div class="space-y-6">
            <%- include('partials/calendar-settings-overview-card', { calendar: calendar }) -%>
            <%- include('partials/calendar-detail-card', { calendar: calendar }) -%>
            <%- include('partials/calendar-bot-settings-card', { calendar: calendar }) -%>
            <%- include('partials/calendar-webhooks-card', { calendar: calendar, webhooks: webhooks }) -%>
          </div>
          
          <!-- Link to Meetings Page -->
          <div class="bg-white rounded-lg shadow p-4">
            <div class="flex items-center justify-between">
              <div>
                <h3 class="text-sm font-medium text-gray-900">Calendar Events</h3>
                <p class="text-xs text-gray-500 mt-1">View and manage your upcoming meetings and recording settings</p>
              </div>
              <a href="/meetings" class="inline-flex items-center rounded-md bg-indigo-600 px-3 py-2 text-sm font-medium text-white shadow-sm hover:bg-indigo-500 transition-colors">
                View Meetings
                <svg class="w-4 h-4 ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7l5 5m0 0l-5 5m5-5H6"></path>
                </svg>
              </a>
            </div>
          </div>
        </div>
      </div>
    </main>
  </div>
</div>
<%- include('partials/footer') -%>
